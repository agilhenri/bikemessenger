<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2e2e2e"
    tools:context=".Menuutama">


    <android.support.v4.view.ViewPager
        android:id="@+id/screenshoot_slider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#202020"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="3dp">
    </android.support.v4.view.ViewPager>

    <android.support.design.widget.FloatingActionButton
    android:id="@+id/fab_logout"
    android:layout_width="60dp"
    android:layout_height="60dp"
    android:layout_gravity="bottom|end"
    android:layout_marginBottom="90dp"
    android:layout_marginRight="@dimen/fab_margin_right"
    android:backgroundTint="@color/fab1_color"
    android:elevation="6dp"
    android:src="@drawable/ic_logout"
    android:visibility="invisible"
    app:pressedTranslationZ="12dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_www"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="160dp"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:backgroundTint="@color/fab1_color"
        android:elevation="6dp"
        android:src="@drawable/ic_action_www"
        android:visibility="invisible"
        app:pressedTranslationZ="12dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_order"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="230dp"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:backgroundTint="@color/fab1_color"
        android:elevation="6dp"
        android:src="@drawable/ic_action_order"
        android:visibility="invisible"
        app:pressedTranslationZ="12dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_plus"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:src="@drawable/ic_action_up"
        android:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:backgroundTint="@color/fab1_color"
        />


</android.support.design.widget.CoordinatorLayout>
